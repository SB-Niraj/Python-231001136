try:
    a = float(input())
    b = float(input())
    
    if b == 0:
        raise ZeroDivisionError("Cannot divide or modulo by zero.")
    
    r = a / b
    print(r)
    
except ZeroDivisionError as z:
    print("Error:", z)
except ValueError:
    print("Error: Non-numeric input provided.")
except Exception as e:
    print("Error occurred:", e)
